<form method="post" action="{{ path('app_part_delete', {'id': part.id}) }}" onsubmit="return confirm('Are you sure you want to delete this bike part?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ part.id) }}">
    <button class="btn btn-danger">Delete <i class="fa fa-trash"></i></button>
</form>